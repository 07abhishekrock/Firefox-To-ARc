{
  "name": "sidebar-src",
  "private": true,
  "version": "0.0.0",
  "type": "commonjs",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "buildWithoutTsc": "vite build",
    "start": "ls src/**/**/*.{tsx,ts} | entr -r pnpm buildWithoutTsc",
